{
  "manifest_version": 2,
  "version": "1.1.0",
  "name": "Cat Tab",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://www.instagram.com/*",
    "http://localhost/",
    "tabs",
    "activeTab"
  ],
  "description": "ADD DESCRIPTION",
  "background": {
    "scripts": ["js/instagramclient.js"],
    "persistent": true
  },
  "content_security_policy":
  "script-src 'self' https://www.instagram.com/; object-src 'self'"
}